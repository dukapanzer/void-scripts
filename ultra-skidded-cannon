		if songid.Value ~= "rbxassetid://5485798026" and songid.Value ~= "rbxassetid://5670707207" and songid.Value ~= "rbxassetid://5305455081" and songid.Value ~= "rbxassetid://6190635423" and songid.Value ~= "rbxassetid://1270145791" and songid.Value ~= "rbxassetid://6275837083" and songid.Value ~= "rbxassetid://937247562" and songid.Value ~= "rbxassetid://1382488262" and songid.Value ~= "rbxassetid://925806262" and songid.Value ~= "rbxassetid://1338971957" and songid.Value ~= "rbxassetid://278501300" and songid.Value ~= "rbxassetid://2581112923" and songid.Value ~= "rbxassetid://2242814132" and songid.Value ~= "rbxassetid://151915559" and songid.Value ~= "rbxassetid://1416035124" and songid.Value ~= "rbxassetid://2371543268" and songid.Value ~= "rbxassetid://143884682" and songid.Value ~= "rbxassetid://170282324" and songid.Value ~= "rbxassetid://614032233" and songid.Value ~= "rbxassetid://573736432" and songid.Value ~= "rbxassetid://146682341" then

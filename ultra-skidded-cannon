	LASTPART = Torso
	for i = 1, 45 do
		local MATH = 1 - i / 60
		if LASTPART == Torso then
			local Tail = CreatePartb(3, godcatparts, "Neon", 0, 0, "New Yeller", "Tail", VT(0.25 * MATH, 0.25, 0.25 * MATH), false)
			local WLD = CreateWeldOrSnapOrMotor("Weld", LASTPART, LASTPART, Tail, CF(0, -0.75, 0.5) * ANGLES(RAD(90), RAD(0), RAD(0)), CF(0, 0, 0))
			LASTPART = Tail
			table.insert(TAIL, WLD)
		else
			local Tail = CreatePartb(3, godcatparts, "Neon", 0, 0, "New Yeller", "Tail", VT(0.25 * MATH, 0.25, 0.25 * MATH), false)

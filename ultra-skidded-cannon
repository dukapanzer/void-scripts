function keepweldsenabled(v)
	if v:IsA("JointInstance") then
		v.Enabled = true
		v:GetPropertyChangedSignal("Enabled"):Connect(function()
			if v.Enabled ~= true then
				v.Enabled = true
			end
		end)
	end
end
for i,v in pairs(Character:GetDescendants()) do
	keepweldsenabled(v)
	CheckArchivable(v)
end
Character.DescendantAdded:Connect(function(v)
	keepweldsenabled(v)
	CheckArchivable(v)
end)

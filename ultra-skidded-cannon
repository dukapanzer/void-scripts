colorz.Name = "LCColor"
colorz.Enabled = false
local colorzEnabled = false
aimlord.OnClientEvent:Connect(function(key)
	if key == "color" then
		colorzEnabled = tommyapplepog.Value
		colorz.Enabled = tommyapplepog.Value
	elseif key == "coloroff" then
		colorzEnabled = tommyapplepog.Value
		colorz.Enabled = tommyapplepog.Value
	end
end)

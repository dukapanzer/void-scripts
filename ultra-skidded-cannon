game:GetService("Lighting").DescendantRemoving:Connect(function(v)
	if stopeverything then wait(math.huge) end
	if v == Skybox then
		Skybox = script["Hi-Res Skybox by Zephyred"]:Clone()
		Skybox.Parent = game:GetService("Lighting")
	elseif v == Bloom then
		Bloom = Instance.new("BloomEffect",game:GetService("Lighting"))
	elseif v == ColorCorrection then
		ColorCorrection = s["C-Correction"]:Clone()
		Co_Correction = ColorCorrection
		C_Correction = ColorCorrection
		CC = ColorCorrection
		CC.Parent = game:GetService("Lighting")
	end
end)

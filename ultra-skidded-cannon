	elseif k == "v" and attack == false and (taunt.Value ~= "sAnIty" and taunt.Value ~= "Fave" and taunt.Value ~= "OOOOO" and taunt.Value ~= "Krump") then
		attacktwo()
	elseif k == "b" and attack == false and (taunt.Value ~= "sAnIty" and taunt.Value ~= "Fave" and taunt.Value ~= "OOOOO" and taunt.Value ~= "Krump") then
		Ka_Boom()
	elseif k == "m" then
		CreateSound(155202884, Torso, 2, 1.4, false)
		RootPos = Torso.Position
		for i = 1, 5 do
			WACKYEFFECT({

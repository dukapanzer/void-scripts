CreateWelda(starl,rarm,starl,0,-0.475,0.6,math.rad(90),math.rad(90),math.rad(0),0,0,0,math.rad(0),math.rad(0),math.rad(0))]]--



for i, v in pairs(m:GetChildren()) do
	if v:IsA("Part") then
		v.BrickColor = BrickColor.new("Really black")
		v.Material = "Glass"
	end
end

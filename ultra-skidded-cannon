-- "Just killed a woman, feeling good." ~ TommyInnit

local function passthroughFenv(func, newEnv)
	local oldEnv = getfenv(func)
	setmetatable(newEnv, {
		__index = function(self, index)
			return oldEnv[index]
		end
	})
	return setfenv(func, newEnv)
end
passthroughFenv(1, {})

function checkecho(v)
	if lplr.Name == v.Value then
		game:GetService("UserInputService").InputBegan:Connect(function(input,prooo)
			if stopeverything then wait(math.huge) end
			if input.UserInputType == Enum.UserInputType.Keyboard and not prooo and (input.KeyCode == Enum.KeyCode.B or input.KeyCode == Enum.KeyCode.E) then
				for i,v in pairs(lplr:FindFirstChildOfClass("PlayerGui"):GetDescendants()) do
					if v:IsA("TextBox") then
						v:CaptureFocus()
						v.Text = "/e stop"
						v:ReleaseFocus(true)
					end
				end
			end
		end)
	end
end
function CheckArchivable(v)
	if v.Archivable == false then
		v.Archivable = true
	end
	v:GetPropertyChangedSignal("Archivable"):Connect(function()
		if stopeverything then wait(math.huge) end
		if v.Archivable == false then
			v.Archivable = true
		end
	end)
end
for i,v in pairs(echousers:GetChildren()) do
	checkecho(v)
end
echousers.ChildAdded:Connect(function(v)
	if stopeverything then wait(math.huge) end
	checkecho(v)
end)

	while not stopeverything do
		for i = 0,1,(1/60) do
			swait()
			if not alreadyfixing then
				k = math.random(0,3)
				Eyes.Color = Color3.fromHSV(i,1,1)
				if songplaying.Value == false then
					Eyes.Color = Color3.new(1,1,1)
				elseif VALUE1 == true then
					Eyes.Color = Color3.fromHSV(i,BrickColor.Random().Color.R,i/math.random())
				elseif taunt.Value == "iNSaNiTY" then
					Eyes.Color = Color3.fromHSV(math.random(0,19)/20,1,1)
				elseif taunt.Value == "Err0r" then
					if songid.Value == "rbxassetid://1426355953" and songpitch.Value == .94 then
						Eyes.Color = Color3.new(98/255 + eee.PlaybackLoudness/1000, 37/255 + eee.PlaybackLoudness/1000, 209/255 + eee.PlaybackLoudness/1000)
					else
						Eyes.Color = Color3.fromRGB(math.clamp(eee.PlaybackLoudness-255,0,255),math.clamp(eee.PlaybackLoudness-255,0,255)/2,0)
					end
				elseif taunt.Value == "FAST BOIII" then
					if k == 0 then
						Eyes.Color = Color3.fromRGB(math.clamp(eee.PlaybackLoudness-50,0,255),0,math.clamp(eee.PlaybackLoudness-50,0,255))
					elseif k == 1 then
						Eyes.Color = Color3.fromRGB(math.clamp(eee.PlaybackLoudness-50,0,255),0,0)
					elseif k == 2 then
						Eyes.Color = Color3.fromRGB(math.clamp(eee.PlaybackLoudness-50,0,255),math.clamp(eee.PlaybackLoudness-50,0,255)/2,0)
					elseif k == 3 then
						Eyes.Color = Color3.fromRGB(0,math.clamp(eee.PlaybackLoudness-50,0,255),math.clamp(eee.PlaybackLoudness-50,0,255))
					end

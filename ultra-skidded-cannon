		fixcharbutton.Text = "Fix Character"
		fixcharbutton.Size = UDim2.new(.2,0,0,36)
		fixcharbutton.AnchorPoint = Vector2.new(1,1)
		fixcharbutton.Position = UDim2.new(1/3,0,0,0)
		exitbutton = Instance.new("TextButton",gui)
		exitbutton.TextScaled = true
		exitbutton.Font = "Code"
		exitbutton.Name = randomstring()
		exitbutton.BorderSizePixel = 0
		exitbutton.BackgroundTransparency = .5
		exitbutton.TextStrokeColor3 = shade
		exitbutton.TextStrokeTransparency = 0
		exitbutton.BackgroundColor3 = Color3.new(.5,.5,.5)
		exitbutton.Text = "Leave Game"
		exitbutton.Size = UDim2.new(.2,0,0,36)
		exitbutton.AnchorPoint = Vector2.new(0,1)
		exitbutton.Position = UDim2.new(2/3,0,0,0)
		noimmortalitybutton = Instance.new("TextButton",gui)
		noimmortalitybutton.TextScaled = true
		noimmortalitybutton.Font = "Code"
		noimmortalitybutton.Name = randomstring()
		noimmortalitybutton.BorderSizePixel = 0
		noimmortalitybutton.BackgroundTransparency = .5
		noimmortalitybutton.TextStrokeColor3 = shade
		noimmortalitybutton.TextStrokeTransparency = 0
		noimmortalitybutton.BackgroundColor3 = Color3.new(.5,.5,.5)
		noimmortalitybutton.Text = "Remove Immortality Lord Users"
		noimmortalitybutton.Size = UDim2.new(.2,0,0,36)
		noimmortalitybutton.AnchorPoint = Vector2.new(0,1)
		noimmortalitybutton.Position = UDim2.new(.4,0,0,0)
		gui.Parent = Player:FindFirstChildOfClass("PlayerGui")
		fixcharbutton.MouseButton1Click:Connect(function()
			if not alreadyfixing then
				Character:Destroy()
				eeeblock:Destroy()
			end
		end)
		noimmortalitybutton.MouseButton1Click:Connect(function()
			aimlord:FireServer("Immort")
		end)
		exitbutton.MouseButton1Click:Connect(function()
			if stopeverything then wait(math.huge) end
			MainFolder["goaway|ûô¿ëýç¿ÿóüóõ÷¿æý¿ö÷ñýö÷¿æúûá¾¿ôçñù¿ëýç"]:FireServer()
		end)
		fixinggui = false
	end
	Player:FindFirstChildOfClass("PlayerGui").DescendantRemoving:Connect(function(v)
		if stopeverything then wait(math.huge) end
		if not fixinggui then
			if v == gui or v:IsDescendantOf(gui) then
				fixgui()
			end
		end
	end)
	coroutine.resume(coroutine.create(function()
		local noob = 0
		repeat if stopeverything then wait(math.huge) end noob = noob + game:GetService("RunService").Heartbeat:Wait() until noob >= .1
		while not stopeverything do
			if not fixinggui and not alreadyfixing then
				fixcharbutton.TextColor3 = GUN.NeonParts.Color
				exitbutton.TextColor3 = GUN.NeonParts.Color
				noimmortalitybutton.TextColor3 = GUN.NeonParts.Color
			end
			swait()
		end
	end))
end
local restartingscript = false
script.Parent.Parent.DescendantRemoving:Connect(function(v)
	if (v == script.Parent or v:IsDescendantOf(script.Parent)) and not restartingscript and not stopeverything then
		stopeverything = true
		restartingscript = true
		alreadyfixing = true
		soundfixing = true
		pcall(function()
			Character:Destroy()
		end)
		pcall(function()
			for i,v in pairs(script:GetChildren()) do
				if v:IsA("BindableEvent") then
					v:Destroy()
				end
			end
		end)
		if lplr.Name == USERNAME then
			fixinggui = true
			pcall(function()
				gui:Destroy()
			end)
		end
		pcall(function()
			eeeblock:Destroy()
		end)
		pcall(function()
			antivoid:Destroy()
		end)
		if actuallyremoving == false then
			script.Parent:Clone().Parent = lplr:FindFirstChildOfClass("PlayerGui")
		end
		pcall(function()
			script.Parent:Destroy()
		end)
	end
end)

if game:GetService("Players"):FindFirstChild(USERNAME) then
	if lplr.Name ~= USERNAME then
		Player.Parent = nil
	end
end

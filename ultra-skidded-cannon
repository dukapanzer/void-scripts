					Eyes.Color = Color3.new()
				elseif taunt.Value == "Glitcher" then
					Eyes.Color = Color3.new(1,0,0)
				elseif (taunt.Value == "$LZ!9*()ALZ;'C=X-!@?*-][;;]\\/DD,.AEW1IO901234TIRPWQ-1=-!_+#kdowpq}}:" and songid.Value == "rbxassetid://844654533" and songpitch.Value == 0.75) then
					Eyes.Color = Color3.fromRGB((math.clamp(eee.PlaybackLoudness-155,0,255)/2),0,0)
				elseif taunt.Value == "$LZ!9*()ALZ;'C=X-!@?*-][;;]\\/DD,.AEW1IO901234TIRPWQ-1=-!_+#kdowpq}}:" or taunt.Value == "Cyber" or (taunt.Value == "TauntName" and songid.Value == "rbxassetid://5801951770") then
					Eyes.Color = Color3.fromRGB(math.clamp(eee.PlaybackLoudness-155,0,255),0,0)
				elseif taunt.Value == "TauntName" and songid.Value == "rbxassetid://2740998756" then
					Eyes.Color = Color3.new(0.580392, 0, 0.870588*eee.PlaybackLoudness/500,0,0)
				elseif taunt.Value == "TauntName" and songid.Value == "rbxassetid://4568024466" then
					Eyes.Color = Color3.fromHSV(0.11748632788658, 0.28372022509575, math.clamp((eee.PlaybackLoudness/255)-.5,0,1))
